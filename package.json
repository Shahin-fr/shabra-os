{
  "name": "shabra-os",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "node scripts/build-with-db-check.js",
    "build:next": "next build",
    "build:full": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "db:analyze": "tsx scripts/optimize-database.ts analyze",
    "db:optimize": "tsx scripts/optimize-database.ts optimize",
    "db:migrate": "node scripts/setup-tables.js",
    "db:generate": "prisma generate",
    "db:studio": "dotenv -e .env.local -- prisma studio",
    "db:setup-local": "dotenv -e .env.local -- node scripts/setup-local-db.js",
    "db:reset-local": "npm run db:setup-local",
    "db:seed": "node scripts/seed-simple.js",
    "db:seed:ts": "dotenv -e .env.local -- ts-node --project tsconfig.seed.json prisma/seed.ts",
    "auth:setup-local": "node scripts/setup-local-auth.js",
    "auth:test": "node scripts/test-auth.js",
    "auth:clear-cache": "node scripts/clear-auth-cache.js",
    "setup:local": "npm run auth:setup-local && npm run db:setup-local",
    "build:clean": "rm -rf .next && rm -rf out",
    "build:analyze-bundle": "ANALYZE=true npm run build",
    "build:validate": "tsx scripts/validate-build-config.ts",
    "bundle:optimize": "tsx scripts/optimize-bundle.ts",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "test:unit": "vitest",
    "test:unit:ui": "vitest --ui",
    "test:unit:coverage": "vitest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test:unit && npm run test:e2e",
    "quality:check": "npm run lint && npm run type-check && npm run test:unit",
    "quality:fix": "npm run lint:fix && npm run format",
    "prepare-docs": "node scripts/prepare-docs.js",
    "seed:wiki": "node prisma/wiki-seed.js",
    "security:audit": "node scripts/security-audit.js",
    "security:generate": "node scripts/generate-secure-credentials.js",
    "security:setup": "npm run security:generate && npm run security:audit",
    "verify:no-console": "node scripts/verify-no-console.js",
    "debug:auth": "node scripts/debug-production-auth.js",
    "postinstall": "prisma generate",
    "prepare": "node -e \"process.env.VERCEL ? console.log('Skipping husky in Vercel environment') : require('child_process').execSync('husky', {stdio: 'inherit'})\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/is-prop-valid": "^1.3.1",
    "@next/bundle-analyzer": "^15.5.2",
    "@prisma/client": "^6.14.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/typography": "^0.5.16",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/bcryptjs": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-jalali": "^4.1.0-0",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.12",
    "gray-matter": "^4.0.3",
    "immer": "^10.1.1",
    "lucide-react": "^0.539.0",
    "next": "15.5.2",
    "next-auth": "^5.0.0-beta.29",
    "node-fetch": "^3.3.2",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-day-picker": "^9.8.1",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "react-markdown": "^10.1.0",
    "recharts": "^3.1.2",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.1",
    "remark-html": "^16.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "terser-webpack-plugin": "^5.3.14",
    "ts-node": "^10.9.2",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@next/eslint-plugin-next": "^15.5.0",
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "dotenv-cli": "^7.4.2",
    "eslint": "^9.34.0",
    "eslint-config-next": "15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-import-helpers": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.8.1",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "prisma": "^6.14.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "prisma": {
    "seed": "ts-node --project tsconfig.seed.json prisma/seed.ts"
  }
}
